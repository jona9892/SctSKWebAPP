@model SctJSKClient.ViewModels.UserOrderDateViewModel

@{
    ViewBag.Title = "ViewOrderedCustomers";
}

<div class="container">
    <table class="table table-hover custab" style="background-color:white">
        <thead>
            <tr>
                <th>Brugernavn</th>
                <th>Navn</th>
                <th>Efternavn</th>
            </tr>
        </thead>
        @foreach (var item in Model.users)
        {
            <tr>
                <td>@item.Username</td>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                <td class="text-center"><a class='btn btn-info btn-xs' href="/Administration/ViewOrderedCustomerDetails?orderdate=@Model.orderdate&id=@item.Id"><span class="glyphicon glyphicon-info-sign"></span> Detaljer</a></td>
            </tr>
        }
    </table>
    <div class="panel-footer">
        <div class="row text-center">
            <div class="col-xs-3">
                <a href="/Administration/Index" class="btn btn-success btn-block"><span class="glyphicon glyphicon-chevron-left"></span>Tilbage</a>
            </div>
        </div>
    </div>
</div>
